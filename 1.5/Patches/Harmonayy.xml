<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd">
       <xpath>/Defs/FactionDef[defName = "Empire"]/permanentEnemyToEveryoneExcept</xpath>
       <value>
           <li>YingletTribeRough</li>
           <li>YingletTribeCivil</li>
           <li>YingletPlayerColony</li>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/PawnRenderTreeDef[defName="Humanlike"]/root/children/li[nodeClass="PawnRenderNode_Head"]</xpath>
        <value>
			<subworkerClasses>
            <li>ShellTooth.Harmony_HeadFix</li>
			</subworkerClasses>
        </value>
    </Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/PawnRenderTreeDef[defName="Humanlike"]/root/children/li[nodeClass="PawnRenderNode_Body"]</xpath>
		<value>
			<subworkerClasses>
				<li>ShellTooth.Harmony_BodyFix</li>
			</subworkerClasses>
		</value>
    </Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/PawnRenderTreeDef[defName="Humanlike"]/root/children/li[nodeClass="PawnRenderNode_Body"]/children/li[tagDef="ApparelBody"]</xpath>
		<value>
			<subworkerClasses>
				<li>ShellTooth.Harmony_ClothesFix</li>
			</subworkerClasses>
		</value>
    </Operation>
	
	<Operation Class="PatchOperationAdd">
       <xpath>/Defs/MeditationFocusDef[defName = "Natural"]/requiredBackstoriesAny</xpath>
       <value>
        <li>
			<categoryName>YingletBackstory</categoryName>
			<slot>Childhood</slot>
		</li>
        </value>
    </Operation>
</Patch>