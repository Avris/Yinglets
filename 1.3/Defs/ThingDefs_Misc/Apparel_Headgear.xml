  <ThingDef ParentName="HatMakeableBase">
    <defName>Yinglet_SlaveCollar</defName>
    <label>slave collar</label>
    <description>An uncomfortable collar made for slaves. It reminds the wearer of their slave status and psychologically suppresses them, reducing the chance of a slave rebellion. Sized to fit around a yinglet's thin neck.</description>
    <graphicData>
      <texPath>Things/Pawn/YingPawn/Apparel/SlaveCollar/SlaveCollar_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.1</Mass>
      <StuffEffectMultiplierArmor>0.0</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.00</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.00</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SlaveSuppressionOffset>0.15</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
      <slaveApparel>true</slaveApparel>
      <bodyPartGroups>
        <li>Neck</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/YingPawn/Apparel/SlaveCollar/SlaveCollar</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
      <hatRenderedAboveBody>true</hatRenderedAboveBody>
      <hatRenderedBehindHead>true</hatRenderedBehindHead>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <defaultOutfitTags>
        <li>Slave</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
